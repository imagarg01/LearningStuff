.PHONY: install run-hello run-tools run-multi run-struct run-stream

install:
	pip install -r requirements.txt

run-hello:
	python hello_world.py

run-tools:
	python custom_tools_agent.py

run-multi:
	python multi_turn_agent.py

run-struct:
	python structured_output_agent.py

run-stream:
	python streaming_agent.py
